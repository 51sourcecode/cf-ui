    "chai": "^3.4.1",
    "chalk": "^1.1.1",
    "defined": "^1.0.0",
    "diff": "^2.2.1",
    "errorify": "^0.3.1",
    "eslint": "^1.8.0",
    "eslint-plugin-cflint": "^1.0.0",
    "express": "^4.13.4",
    "gulp": "^3.9.0",
    "gulp-babel": "^6.1.1",
    "gulp-eslint": "^1.1.1",
    "gulp-newer": "^1.1.0",
    "gulp-plumber": "^1.0.1",
    "gulp-util": "^3.0.7",
    "gulp-watch": "^4.3.5",
    "isparta": "^4.0.0",
