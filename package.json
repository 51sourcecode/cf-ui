{
  "private": true,
  "name": "cf-ui",
  "description": "CloudFlare UI Framework",
  "author": "James Kyle <jkyle@cloudflare.com>",
  "license": "BSD-3-Clause",
  "scripts": {
    "build": "gulp build",
    "bootstrap": "lerna bootstrap -i",
    "dev": "gulp dev",
    "examples": "browserify packages/cf-component-*/example/*/component.js -o example/bundle.js -t babelify && node example/server.js",
    "examples-dev": "watchify packages/cf-component-*/example/*/component.js -o example/bundle.js -t babelify -p errorify & node example/server.js",
    "lint": "gulp lint",
    "publish": "lerna publish -i",
    "test": "gulp test",
    "updated": "lerna updated -i"
  },
  "devDependencies": {
    "babel-core": "^6.0.20",
    "babel-eslint": "^4.1.6",
    "babel-preset-cf": "^1.2.0",
    "babelify": "^7.2.0",
